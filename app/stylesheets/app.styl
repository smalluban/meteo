@import 'nib'

// global-reset()
// reset-html5()

// Apply a na tural box layout model to all elements.
// http://paulirish.com/2012/box-sizing-border-box-ftw/
*
  box-sizing: border-box
  -webkit-tap-highlight-color rgba(0,0,0,0)

html
  //font 62.5% 'Lato', serif

html, body
  width 100%
  height 100%
  overflow hidden
  background white
  color #34495E

a
  color inherit
  text-decoration none